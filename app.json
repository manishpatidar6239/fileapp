{
  "expo": {
    "name": "Atulit Computer",
    "slug": "Atulit Computer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/01.png",
    "scheme": "Atulit Computer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/01.png",
        "backgroundColor": "#002060"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.atulit"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/01.png", 
          "imageWidth": 100,
          "contentFit": "contain",
          "backgroundColor": "#002060",
          "dark": {
            "backgroundColor": "#002060"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
